###########################################################################
# pbench "official" repos
# Internal test repo
[pbench-test]
name=Pbench test $releasever - $basearch
baseurl=http://pbench.perf.lab.eng.bos.redhat.com/test-repo/{{ pbench_configuration_environment }}/$releasever/
arch=$basearch
enabled=0
gpgcheck=0
skip_if_unavailable=1

# External COPR repo
[copr-pbench-test]
name=Copr test repo for pbench
baseurl={{ testbaseurl }}
skip_if_unavailable=True
gpgcheck={{ gpgcheck }}
gpgkey={{ testgpgkey }}
enabled=0
enabled_metadata=1
skip_if_unavailable=1
